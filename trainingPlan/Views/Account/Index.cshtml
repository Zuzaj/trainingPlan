@model IEnumerable<trainingPlan.Models.User>

@{
    ViewData["Title"] = "User List";
}

<h2>User List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Password Hash</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.PasswordHash</td>
            </tr>
        }
    </tbody>
</table>

<a href="@Url.Action("Register", "Account")" class="btn btn-primary">Add New User</a>
